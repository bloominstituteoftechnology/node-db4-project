    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "jest": "^27.0.4",
    "knex-cleaner": "^1.3.1",
    "supertest": "^6.1.3"
  }
