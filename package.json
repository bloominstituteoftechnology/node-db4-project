    "helmet": "^5.0.2",
    "knex": "^1.0.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "knex-cleaner": "^1.3.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
