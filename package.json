    "express": "^4.17.1",
    "knex": "^0.95.11",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "helmet": "^4.6.0",
    "knex-cleaner": "^1.3.1",
    "nodemon": "^2.0.12"
  }
}
